{"code":"import { creepHelpers } from 'helpers.role';\r\nimport { roleBuilder } from 'role.builder';\r\nexport var roleRepairer = {\r\n    run: function (creep) {\r\n        if (creep.memory.working == true && creep.carry.energy == 0) {\r\n            creep.memory.working = false;\r\n        }\r\n        else if (creep.memory.working == false && creep.carry.energy == creep.carryCapacity) {\r\n            creep.memory.working = true;\r\n        }\r\n        if (creep.memory.working == true) {\r\n            var structure = creep.pos.findClosestByPath(FIND_STRUCTURES, {\r\n                filter: function (s) { return s.hits < s.hitsMax && s.structureType != STRUCTURE_WALL && s.structureType != STRUCTURE_RAMPART; }\r\n            });\r\n            if (structure) {\r\n                if (creep.repair(structure) == ERR_NOT_IN_RANGE) {\r\n                    creep.moveTo(structure);\r\n                }\r\n            }\r\n            else {\r\n                roleBuilder.run(creep);\r\n            }\r\n        }\r\n        else {\r\n            creepHelpers.extractFromContainer(creep);\r\n        }\r\n    }\r\n};\r\n//# sourceMappingURL=role.repairer.js.map","map":{"version":3,"file":"role.repairer.js","sourceRoot":"","sources":["src/role.repairer.ts"],"names":[],"mappings":"AAAA,OAAO,EAAC,YAAY,EAAC,MAAM,cAAc,CAAC;AAC1C,OAAO,EAAC,WAAW,EAAC,MAAM,cAAc,CAAC;AAEzC,MAAM,CAAC,IAAM,YAAY,GAAG;IACxB,GAAG,EAAE,UAAC,KAAY;QACd,IAAI,KAAK,CAAC,MAAM,CAAC,OAAO,IAAI,IAAI,IAAI,KAAK,CAAC,KAAK,CAAC,MAAM,IAAI,CAAC,EAAE;YACzD,KAAK,CAAC,MAAM,CAAC,OAAO,GAAG,KAAK,CAAC;SAChC;aACI,IAAI,KAAK,CAAC,MAAM,CAAC,OAAO,IAAI,KAAK,IAAI,KAAK,CAAC,KAAK,CAAC,MAAM,IAAI,KAAK,CAAC,aAAa,EAAE;YACjF,KAAK,CAAC,MAAM,CAAC,OAAO,GAAG,IAAI,CAAC;SAC/B;QAED,IAAI,KAAK,CAAC,MAAM,CAAC,OAAO,IAAI,IAAI,EAAE;YAC9B,IAAI,SAAS,GAAG,KAAK,CAAC,GAAG,CAAC,iBAAiB,CAAC,eAAe,EAAE;gBACzD,MAAM,EAAE,UAAC,CAAC,IAAK,OAAA,CAAC,CAAC,IAAI,GAAG,CAAC,CAAC,OAAO,IAAI,CAAC,CAAC,aAAa,IAAI,cAAc,IAAK,CAAC,CAAC,aAAa,IAAI,iBAAiB,EAAhG,CAAgG;aAClH,CAAC,CAAC;